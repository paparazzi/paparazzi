{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "(lldb) Flamewheel simsitl",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/var/aircrafts/Flamewheel_ctl_alloc/nps/simsitl",
			"env": {"PAPARAZZI_HOME": "${workspaceFolder}"},
			"args": [],
			"cwd": "${workspaceFolder}",
			"relativePathBase": "${workspaceFolder}"
		},
		{
			"name": "(gdb) Flamewheel simsitl",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/var/aircrafts/Flamewheel_ctl_alloc/nps/simsitl",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [{"name":"PAPARAZZI_HOME", "value": "${workspaceFolder}"}],
			"externalConsole": false,
			"MIMode": "gdb"/*,
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description":  "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
			*/
		}

	]
}