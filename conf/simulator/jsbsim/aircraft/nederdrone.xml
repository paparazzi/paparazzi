                <property>fcs/m10_lag</property>
                <property>fcs/m12_lag</property>
              </sum>
              <value> -1 </value>
            </product>
          </sum>
          <value> 1.2 </value>
        </product>
      <location unit="M">
        <x>0</x>
        <y>1</y>
        <z>0</z>
      </location>
      <direction>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
