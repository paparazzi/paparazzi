<!DOCTYPE module SYSTEM "module.dtd">

<module name="rssi" dir="multi">
  <doc>
    <description>stores received rssi values for communication protocols that support it</description>
  </doc>
  <header>
    <file name="rssi.h"/>
  </header>
  <init fun="rssi_init()"/>
  <datalink message="RSSI" fun="parse_rssi_dl()"/>
  <makefile>
    <file name="rssi.c"/>
  </makefile>
</module>

