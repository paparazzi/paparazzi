<!DOCTYPE module SYSTEM "module.dtd">

<module name="GUIDED_FlightPlan" dir="flight_plan_guided">
  <doc>
    <description>GUIDED_FlightPlan</description>
  </doc>

  <settings>
    <dl_settings>
      <dl_settings NAME="guided_flight_plan">
        <dl_setting var="nom_flight_alt" min="0.2" step="0.1" max="3." module="flight_plan_guided/flight_plan_guided" shortname="nom_flight_alt" param="NOM_FLIGHT_ALT"/>
    </dl_settings>
    </dl_settings>
  </settings>

  <header>
    <file name="flight_plan_guided.h"/>
  </header>

  <init fun="flight_plan_guided_init()"/>

  <makefile target="ap">
    <file name="flight_plan_guided.c"/>
  </makefile>

  <makefile target="nps">
    <file name="flight_plan_guided.c"/>
  </makefile>

</module>
