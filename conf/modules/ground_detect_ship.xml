<!DOCTYPE module SYSTEM "module.dtd">
<module name="ground_detect_ship" dir="nav">
  <doc>
    <description>Ground detection module relying on lidar to detect ground on a ship</description>
  </doc>
  <header>
    <file name="ground_detect_ship.h"/>
  </header>
  <init fun="ground_detect_ship_init()"/>
  <periodic fun="ground_detect_periodic()" freq="50"/>
  <makefile>
    <file name="ground_detect_ship.c"/>
    <test>
      <define name="INDI_OUTPUTS" value="4"/>
      <define name="INDI_NUM_ACT" value="4"/>
      <define name="PERIODIC_FREQUENCY" value="512"/>
    </test>
  </makefile>
</module>
