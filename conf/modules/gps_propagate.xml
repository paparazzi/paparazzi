<!DOCTYPE module SYSTEM "module.dtd">
<module name="gps_propagate" dir="gps" task="sensors">
  <doc>
    <description>Continue flying on baro and magnetometer when GPS fails</description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings NAME="gps_propagate">
        <dl_setting var="extrapolation_method" min="0" step="1" max="2" shortname="extrap method" values="HEADING_BIAS|WIND_VECTOR|RLS"/>
        <dl_setting var="lambda" min="0" step="0.001" max="1" shortname="forgetting factor"/>
        <dl_setting var="wind_estimation_enabled" min="0" step="1" max="1" shortname="RLS update" values="FALSE|TRUE"/>
      </dl_settings>
    </dl_settings>
  </settings>
  <dep>
  </dep>
  <header>
    <file name="gps_propagate.h"/>
  </header>
  <init fun="gps_propagate_init()"/>
  <periodic fun="gps_propagate_periodic()" freq="10.0" autorun="TRUE"/>
  <makefile>
    <file name="gps_propagate.c"/>
    <test/>
  </makefile>
</module>
