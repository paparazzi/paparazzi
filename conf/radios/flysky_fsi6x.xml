  <channel function="YAW" min="1100" neutral="1500" max="1900" average="0"/>	        <!-- left stick left/right-->
  <channel function="KILL_SWITCH" min="1100" neutral="1500" max="1900" average="1"/>    <!-- switch (channel 5) -->
  <channel function="AUX2" min="1100" neutral="1500" max="1900" average="1"/>           <!-- switch (channel 6) -->
  <channel function="MODE" min="1100" neutral="1500" max="1900" average="1"/>           <!-- switch (channel 7) - three phase -->
  <channel function="AUX3" min="1100" neutral="1500" max="1900" average="1"/>           <!-- switch (channel 8) -->
