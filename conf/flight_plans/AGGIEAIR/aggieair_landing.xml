<!DOCTYPE procedure SYSTEM "../flight_plan.dtd">

<procedure>
<blocks>
    <block name="Landing CW">
      <call_once fun="nav_skid_landing_setup(WP_AF, WP_TD, nav_radius)"/>
      <call fun="nav_skid_landing_run()"/>
    </block>
    <block name="Landing CCW">
      <call_once fun="nav_skid_landing_setup(WP_AF, WP_TD, -nav_radius)"/>
      <call fun="nav_skid_landing_run()"/>
    </block>
</blocks>
</procedure>
