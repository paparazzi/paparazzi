    <field name="a1"  type="int16"/>
    <field name="a2"  type="int16"/>
    <field name="a3"  type="int16"/>
    <field name="a4"  type="uint16"/>
    <field name="a5"  type="uint16"/>
    <field name="a6"  type="uint16"/>
    <field name="b1"  type="int16"/>
    <field name="b2"  type="int16"/>
