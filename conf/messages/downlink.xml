  </message>

  <message name="ESTIMATOR" id="14">
    <field name="z" type="float" unit="m"/>
    <field name="z_dot" type="float" unit="m/s"/>
  </message>

  <message name="PAYLOAD" id="15">
    <field name="values" type="uint8[]" unit="none"/>
  </message>

  <message name="WIND_INFO_RET" id="16">
    <field name="east" type="float" unit="m/s"/>
    <field name="north" type="float" unit="m/s"/>
    <field name="airspeed" type="float" unit="m/s"/>
  </message>

  <message name="CHRONO" id="17">
    <field name="tag"  type="uint8"/>
    <field name="time" type="uint32" unit="us"/>
  </message>

  <message name="TIME" id="18">
    <field name="t" type="uint32"/>
  </message>


<!-- _________________________________ BATTERY / ENERGY / MOTOR ___________________________________ -->

  <message name="ENERGY" id="20">
    <field name="voltage" type="uint16" unit="1e-2V" alt_unit="V" alt_unit_coef="0.01"/>
    <field name="current" type="int16" unit="1e-2A" alt_unit="A" alt_unit_coef="0.01"/>
    <field name="power"   type="int16" unit="1e-1W" alt_unit="W" alt_unit_coef="0.1"/>
    <field name="energy" type="int16" unit="mAh"/>
    <field name="throttle" type="int16" unit="pprz"/>
  </message>

  <message name="MPPT" id="21">
    <field name="values" type="int16[]" alt_unit_coef="1e-3"/>
  </message>

  <message name="MOTOR" id="22">
    <field name="rpm" type="uint16"/>
    <field name="current" type="int32" unit="mA"/>
  </message>

<!-- _________________________________________ COMMANDS ___________________________________________ -->
  <message name="COMMANDS" id="25">
    <field name="values" type="int16[]" unit="ticks"/>
  </message>

  <message name="ROTORCRAFT_CMD" id="26">
    <field name="cmd_roll"   type="int32"/>
    <field name="cmd_pitch"  type="int32"/>
    <field name="cmd_yaw"    type="int32"/>
    <field name="cmd_thrust" type="int32"/>
  </message>
<!-- _________________________________________ RC / PPM ___________________________________________ -->
  <message name="PPM" id="28">
    <field name="ppm_rate" type="uint8"/>
    <field name="values" type="uint16[]" unit="usec"/>
  </message>

  <message name="RC" id="29">
    <field name="values" type="int16[]" unit="pprz" format="%d"/>
  </message>

  <message name="ROTORCRAFT_RADIO_CONTROL" id="30">
    <field name="roll"     type="int16" unit="pprz"/>
    <field name="pitch"    type="int16" unit="pprz"/>
    <field name="yaw"      type="int16" unit="pprz"/>
    <field name="throttle" type="int16" unit="pprz"/>
    <field name="mode"     type="int16" unit="pprz"/>
    <field name="kill"     type="int16" unit="pprz"/>
    <field name="status"   type="uint8" values="OK|LOST|REALLY_LOST"/>
  </message>

<!-- ___________________________________________ GPS ______________________________________________ -->

  <message name="GPS_UTM" id="33">
    <field name="fix"       type="uint8"  unit="byte_mask" values="NONE|UKN1|UKN2|3D"/>
