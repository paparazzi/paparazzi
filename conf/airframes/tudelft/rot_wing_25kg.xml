        <define name="USE_AIRSPEED"                     value="TRUE"/>
        <define name="NAV_HYBRID_MAX_DECELERATION"      value="0.5"/>
        <define name="MULTI_GPS_MODE"                   value="GPS_MODE_PRIMARY"/>
