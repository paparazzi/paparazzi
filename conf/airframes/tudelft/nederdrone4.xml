    <define name="MPU_CHAN_X" value="1"/>
    <define name="MPU_CHAN_Y" value="0"/>
    <define name="MPU_CHAN_Z" value="2"/>
    <define name="MPU_X_SIGN" value="1"/>
    <define name="MPU_Y_SIGN" value="1"/>
    <define name="MPU_Z_SIGN" value="-1"/>

