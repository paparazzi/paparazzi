    <define name="MAG_Z_SENS" value="0.6352539557433349" integer="16"/>

    <!-- Define axis in hover frame -->
    <define name="BODY_TO_IMU_PHI"   value="0." unit="deg"/>
    <define name="BODY_TO_IMU_THETA" value="82.8" unit="deg"/>
